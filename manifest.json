{
  "manifest_version": 2,
  "name": "Branch name from JIRA ticket",
  "version": "1.0",
  "description": "Chrome Extension for creating a branch name from ticket number and ticket title",
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
		"default_title": "Permissions Example",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png"
    }
  },
	"permissions": [
		"tabs",
		"activeTab",
		"clipboardWrite"
	]
}
